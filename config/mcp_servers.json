{
  "shodan-mcp": {
    "id": "shodan-mcp",
    "name": "Shodan MCP Server",
    "command": "uvx",
    "args": [
      "mcp-shodan@latest"
    ],
    "env": {
      "SHODAN_API_KEY": ""
    },
    "capabilities": [
      "ip_lookup",
      "host_search",
      "vulnerability_search"
    ],
    "api_keys_required": [
      "SHODAN_API_KEY"
    ],
    "rate_limits": {
      "requests_per_minute": 100
    },
    "timeout": 30000,
    "retry_count": 3,
    "enabled": true
  },
  "virustotal-mcp": {
    "id": "virustotal-mcp",
    "name": "VirusTotal MCP Server",
    "command": "uvx",
    "args": [
      "mcp-virustotal@latest"
    ],
    "env": {
      "VIRUSTOTAL_API_KEY": ""
    },
    "capabilities": [
      "file_scan",
      "url_scan",
      "ip_report",
      "domain_report"
    ],
    "api_keys_required": [
      "VIRUSTOTAL_API_KEY"
    ],
    "rate_limits": {
      "requests_per_minute": 500
    },
    "timeout": 30000,
    "retry_count": 3,
    "enabled": true
  },
  "nuclei-mcp": {
    "id": "nuclei-mcp",
    "name": "Nuclei MCP Server",
    "command": "uvx",
    "args": [
      "nuclei-mcp@latest"
    ],
    "env": {
      "NUCLEI_TEMPLATES_PATH": "./nuclei-templates"
    },
    "capabilities": [
      "vulnerability_scan",
      "template_scan",
      "custom_scan"
    ],
    "api_keys_required": [],
    "rate_limits": {
      "concurrent_scans": 10
    },
    "timeout": 30000,
    "retry_count": 3,
    "enabled": true
  },
  "bloodhound-mcp": {
    "id": "bloodhound-mcp",
    "name": "BloodHound MCP Server",
    "command": "uvx",
    "args": [
      "bloodhound-mcp@latest"
    ],
    "env": {
      "NEO4J_URI": "bolt://localhost:7687",
      "NEO4J_USER": "neo4j",
      "NEO4J_PASSWORD": "bloodhound"
    },
    "capabilities": [
      "ad_analysis",
      "attack_paths",
      "privilege_escalation"
    ],
    "api_keys_required": [],
    "rate_limits": {
      "queries_per_minute": 1000
    },
    "timeout": 30000,
    "retry_count": 3,
    "enabled": true
  },
  "ghidra-mcp": {
    "id": "ghidra-mcp",
    "name": "Ghidra MCP Server",
    "command": "uvx",
    "args": [
      "ghidra-mcp@latest"
    ],
    "env": {
      "GHIDRA_HEADLESS_PATH": "./tools/ghidra/support/analyzeHeadless"
    },
    "capabilities": [
      "binary_analysis",
      "decompilation",
      "function_analysis"
    ],
    "api_keys_required": [],
    "rate_limits": {
      "concurrent_analysis": 2
    },
    "timeout": 30000,
    "retry_count": 3,
    "enabled": true
  }
}